<%= form_with model: [:admin, @financial_taxon] do |f| %>
  <%= render 'shared/error_messages', target: @financial_taxon %>
  <%= f.text_field :name %>
  <%= render partial: 'taxon/nodes/siblings_select', locals: { f: f, node: @financial_taxon_root } %>
  <%= f.check_box :take_stock %>
  <%= f.check_box :individual %>
  <%= f.submit %>
<% end %>
