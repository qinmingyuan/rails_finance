<% form_object = form_object(@expense) %>

<% if params[:item] == 'member' %>
document.getElementById('expense_expense_members_<%= params[:index] %>').insertAdjacentHTML('afterend', '<%= j(render partial: 'member_field', locals: { f: form_object, index: UidHelper.sec_uuid }) %>')
<% elsif params[:item] == 'item' %>
document.getElementById('expense_expense_items_<%= params[:index] %>').insertAdjacentHTML('afterend', '<%= j(render partial: 'item_field', locals: { f: form_object, index: UidHelper.sec_uuid }) %>')
<% end %>
